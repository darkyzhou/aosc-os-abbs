                 --without-system-et \
                 --without-system-ss \
                 --without-tcl \
                 --enable-dns-for-realm \
                 --with-ldap \
                 --without-system-verto \
                 --without-libedit \
                 --disable-aesni"

RECONF=0
NOPARALLEL=1
