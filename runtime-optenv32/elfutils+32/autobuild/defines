PKGNAME=elfutils+32
PKGSEC=utils
PKGDEP="bzip2+32 zlib+32"
BUILDDEP="devel-base+32"
PKGDES="Libraries and utilities to handle ELF object files and DWARF debugging information (32-bit x86 runtime)"

